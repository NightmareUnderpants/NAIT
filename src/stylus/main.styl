@import 'blocks/games'
@import 'blocks/navbar'

@import 'gallery'
@import 'mixins'
@import 'variables'

body
  background #202020
  color #fff

a
  color #fff

a:visited
  color #aaa

.navbar
  border-top 3px double #fff
  border-bottom 3px double #fff

.card
  background #1a1a1a
  border-color rgba(255,255,255,.18)
  color #fff

.card-soft
  border-radius 8px

.avatar
  width 72px
  aspect-ratio 1 / 1
  object-fit cover
  border-radius 50%
  display block
  margin 0 auto 10px

